{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "unified-fabric-data-factory"
		},
		"AzureGoldSql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureGoldSql'"
		},
		"fabric_bronze_blob_storage_link_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'fabric_bronze_blob_storage_link'"
		},
		"AzureGoldSql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "fabric-gold-sql.database.windows.net"
		},
		"AzureGoldSql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "fabric-gold"
		},
		"AzureGoldSql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "unified-fabric-data-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureGoldSql_password": {
						"value": "[parameters('AzureGoldSql_password')]"
					},
					"fabric_bronze_blob_storage_link_connectionString": {
						"value": "[parameters('fabric_bronze_blob_storage_link_connectionString')]"
					},
					"AzureGoldSql_properties_typeProperties_server": {
						"value": "[parameters('AzureGoldSql_properties_typeProperties_server')]"
					},
					"AzureGoldSql_properties_typeProperties_database": {
						"value": "[parameters('AzureGoldSql_properties_typeProperties_database')]"
					},
					"AzureGoldSql_properties_typeProperties_userName": {
						"value": "[parameters('AzureGoldSql_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "unified-fabric-data-factory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'unified-fabric-data-factory_ArmTemplate_0')]"
			]
		},
		{
			"name": "unified-fabric-data-factory_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'unified-fabric-data-factory_ArmTemplate_1')]"
			]
		}
	]
}