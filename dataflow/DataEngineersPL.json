{
	"name": "DataEngineersPL",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Json1",
						"type": "DatasetReference"
					},
					"name": "DataEngineers"
				},
				{
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "flatten1"
				},
				{
					"name": "filter1"
				},
				{
					"name": "sort1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          id as integer,",
				"          name as string,",
				"          address as (city as string, zip as string),",
				"          skills as string[],",
				"          salary as integer,",
				"          active as boolean",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: true,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'arrayOfDocuments') ~> DataEngineers",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> source1",
				"DataEngineers foldDown(unroll(skills),",
				"     mapColumn(",
				"          id,",
				"          name,",
				"          address,",
				"          skills,",
				"          salary,",
				"          active",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten1",
				"flatten1 filter(active == true()) ~> filter1",
				"filter1 sort(desc(salary, true)) ~> sort1"
			]
		}
	}
}